{
  "name": "continua11y-sitemapper",
  "version": "0.1.0",
  "description": "Continua11y report generation for sites with public pages that should be scraped via a sitemap",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha --recursive",
    "lint": "node_modules/.bin/jshint index.js lib/ test/",
    "test_coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --recursive"
  },
  "bin": {
    "continua11y-sitemapper": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/node-continua11y-sitemapper.git"
  },
  "keywords": [
    "accessibility",
    "sitemap",
    "report",
    "ci"
  ],
  "author": "baccigalupi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/node-continua11y-sitemapper/issues"
  },
  "homepage": "https://github.com/18F/node-continua11y-sitemapper#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.4.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "pa11y-ci": "^0.3.0"
  }
}
